                            <a href="/page/player_balance"><%= __('menu.balance.history') %></a>
                        </li>
                        <li class="<%= (page == 'player_transaction')?'active':''%>">
                            <a href="/page/player_transaction"><%= __('menu.game.history') %></a>
                        </li>
                    </ul>
                </li>
                <li class="has-sub">
                    <a class="js-arrow <%= (page == 'currency'||page == 'agent'||page == 'user'||page == 'provider'||page == 'game')?'open':''%>" href="#">
                        <i class="fas fa-line-chart"></i><%= __('menu.statistics') %></a>
                    <ul class="list-unstyled navbar__sub-list js-sub-list" style="display: <%= (page == 'currency'||page == 'agent'||page == 'user'||page == 'provider'||page == 'game')?'block':'none'%>">
                        <li class="<%= (page == 'currency')?'active':''%>">
                            <a href="/page/currency-stat" ><%= __('menu.currency') %></a>
                        </li>
                        <li class="<%= (page == 'agent')?'active':''%>">
                            <a href="/page/agent-stat"><%= __('menu.agent') %></a>
                        </li>
                        <li class="<%= (page == 'user')?'active':''%>">
                            <a href="/page/user-stat"><%= __('menu.user') %></a>
